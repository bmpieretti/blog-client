{
  "presets": [[
    "@babel/preset-env", {
      "modules": false
    }
  ], "@babel/preset-react"],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "fileName": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", { "modules": "commonjs" }]
      ]
    }
  }
}