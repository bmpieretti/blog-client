{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "document": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "import/no-extraneous-dependencies": 0,
    "no-unused-expressions": [
      "warn", {
        "allowTaggedTemplates": true
      }
    ]
  },
  "env": {
    "jest": true
  }
}